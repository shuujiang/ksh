.keshihua
	.chartData
			width 100%
			height 890px
			.chartData-zuo
				width 160px
				height 100%
				float left
				padding 20px
				border 1px solid #ddd
				overflow hidden
				.chartData-zuo-1
					width 180px
					height 100%
					overflow-y auto
					.dataTable
						width 120px
						height 90px
						font-size 12px
						border-bottom 2px solid #f5f5f5
						.shujuyuan
							width 100%
							height 30px
							margin-bottom 10px
							span
								font-size 18px
							select
								padding-left 10px
								font-size 14px
								width 70px
								height 25px
					.dimension
						width 120px
						font-size 14px
						.dimension_x
							overflow hidden
							height 50%
							border-bottom 1px solid #f5f5f5
						.dimension_y
							overflow hidden
							height 50%
						.variate
							font-size 12px
							width 120px
							height 40px
							line-height 40px
							float left
							padding-left 10px
							padding-right 10px
							cursor move
							overflow hidden
							&:hover
								width: 120px
								background-color  #ccc
						.kongbai
							width 100%
							height 40px
			.chartData-zhong
				height: 100%
				margin-left 160px
				overflow-x hidden
				overflow-y auto
				.dimension
					width 1760px
					height 40px
					border-left 1px solid #f5f5f5
					border-right 1px solid #f5f5f5
					border-bottom 1px solid #bbb
					span
						width 199px
						height 39px
						text-align center
						color #636363
						line-height 38px
						float left
						border-right 1px solid #bbb
					.X_variable,
					.Y_variable
						width 1230px
						height 38px
						float left
						.variate
							cursor move
							float left
							height 38px
							padding-left 20px
							line-height 38px
				.chartData_graph
					width 100%
					border-right 1px solid #f5f5f5
					background-color #ddd
					._graph-left
						display none
						width 200px
						height 100%
						float left
						border-right 1px solid #aaa
					._graph-right
						height 100%
						padding 40px 20px
						._graph
							width 100%
							height 100%
							background-color white
							.aBox
								width 100%
								height 100%
								position relative
								._tuozhai
									width 100%
									height 100%
									border-width 1px
									overflow hidden
			.chartData-you
				display none
				width 320px
				height 100%
				float right
				overflow hidden
				.chartData-you-1
					float left
					width 340px
					height 100%
					overflow-y auto
					overflow-x hidden
					.settings
						width 320px
						padding 10px
						overflow hidden
						.configuration
							width 100%
							.titleName
								float left
								width 100%
								height 30px
								line-height 30px
							.subtype
								float left
								width 100%
								height 200px
								div
									float left
									width 46px
									height 46px
									text-align center
									border 1px solid transparent
									margin 2px
									&:hover
										border-color #2d68a2
										background-color #ddd
									i
										display inline-block
										width 40px
										height 40px
										margin 2px
								.pitchoOn
									border-color #2d68a2
									background-color #ddd
							.function
								float left
								width 100%
								padding 0px 5px
								span
									display inline-block
									width 100%
									height 25px
									line-height 25px
									color #777
								.branches
									color #888
									padding-left 20px
									.branches1
										display none
										padding-top 5px
										select
											margin-right 5px
											float left
											width 40px
											height 25px
										input
											margin-right 5px
											float left
											padding-left 10px
											width 40px
											height 25px
										span
											margin-right 5px
											float left
											padding-left 10px
											width 40px
											height 25px
